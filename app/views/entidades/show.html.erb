<%- model_class = Entidade -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @entidade.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cnpj) %>:</strong></dt>
  <dd><%= @entidade.cnpj %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @entidade.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @entidade.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estado) %>:</strong></dt>
  <dd><%= @entidade.estado %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cidade) %>:</strong></dt>
  <dd><%= @entidade.cidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bairro) %>:</strong></dt>
  <dd><%= @entidade.bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cep) %>:</strong></dt>
  <dd><%= @entidade.cep %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rua) %>:</strong></dt>
  <dd><%= @entidade.rua %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero) %>:</strong></dt>
  <dd><%= @entidade.numero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>
  <dd><%= @entidade.telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contato) %>:</strong></dt>
  <dd><%= @entidade.contato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @entidade.status %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ramo_id) %>:</strong></dt>
  <dd><%= @entidade.ramo_id %></dd>
</dl>

<%= link_to t('Voltar'),
              entidades_path, :class => 'btn btn-default'  %>
<%= link_to t('Editar'),
              edit_entidade_path(@entidade), :class => 'btn btn-default' %>
<%= link_to t('Excluir'),
              entidade_path(@entidade),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'VocÃª tem certeza?')) },
              :class => 'btn btn-danger' %>
