<%- model_class = ProfissionalServico -%>
<div class="page-header">
  <h1>Vínculo</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:profissional_id) %>:</strong></dt>
  <dd><%= @profissional_servico.profissional.nome %></dd>
  <dt><strong>Serviço:</strong></dt>
  <dd><%= @profissional_servico.lista_preco_id %></dd>
</dl>

<%= link_to t('Voltar'),
              profissional_servicos_path, :class => 'btn btn-default'  %>
<%= link_to t('Editar'),
              edit_profissional_servico_path(@profissional_servico), :class => 'btn btn-default' %>
<%= link_to t('Excluir'),
              profissional_servico_path(@profissional_servico),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Você tem certeza?')) },
              :class => 'btn btn-danger' %>
